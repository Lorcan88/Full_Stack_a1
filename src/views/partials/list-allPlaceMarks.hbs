{{#each placeMarks}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{title}}
    </h2>
    <p> Total Number of PlaceMarks: {{placMark.size}} </p>
    <a href="/placeMark/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-folder-open"></i>
      </span>
    </a>
    <a href="/adminDashboard/deleteplaceMark/{{id}}" class="button">
      <i class="fas fa-trash"></i>
    </a>
  </div>
{{/each}}

