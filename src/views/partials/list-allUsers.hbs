{{#each user}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{firstName}}
      {{lastName}}
    </h2>
    <p> Total Number of PlaceMarks: {{placeMarks.place.length}} </p>
    <a href="/adminUserViewDashboard/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-folder-open"></i>
      </span>
    </a>
    <a href="/adminDashboard/deleteuser/{{_id}}" class="button">
      <i class="fas fa-trash"></i>
    </a>
  </div>
{{/each}}